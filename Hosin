username = 'Amir Hosin'
SAMPMENU = 1

gg.toast("Please wait ⏳")

gg.setVisible(true)

function EXIT()
  gg.setVisible(true)
  gg.toast("EXIT🚫")
  gg.clearResults()
  os.exit()
end

function silent()
  WH1 = gg.choice({
    "🧍𝐒𝐢𝐥𝐞𝐧𝐭 𝐀𝐢𝐦🧍",
    "  🔙 BACK 🔙",
  }, nil, "\n ⚕️Creator : @Cemetery10\n 💥Channel : @Silent_Aim_Pkn")
  if WH1 == 1 then
    hahaha = gg.choice({
        '⌜ ᴏɴ ⌟',
        '⌜ ᴏғғ ⌟',
        ' ⟪ ʙᴀᴄᴋ ⟫ ' }, nil,
      '◤                                                       ◥\n      ◄ [ silent aim (10) Degree ] ►               \n◣                                                       ◢')

    if hahaha == 1 then silentHead() end
    if hahaha == 2 then silentHeadoff() end
    if hahaha == 3 then silent() end
  end

  if WH1 == 2 then HOME() end
  SAMPMENU = -1
end

function silentHead()
  gg.processResume()
  gg.searchNumber("1,043,878,380", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1, 0)
  gg.processResume()
  gg.getResults(400, nil, nil, nil, nil, nil, nil, nil, nil)
  gg.processResume()
  gg.editAll("1,055,350,350", gg.TYPE_DWORD)
  gg.toast('[' .. username .. ']' .. " Cheat Deactive✅")
  gg.clearResults()
  silent()
end

function silentHeadoff()
  gg.processResume()
  gg.searchNumber("1,055,350,350", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1, 0)
  gg.processResume()
  gg.getResults(400, nil, nil, nil, nil, nil, nil, nil, nil)
  gg.processResume()
  gg.editAll("1,043,878,380", gg.TYPE_DWORD)
  gg.toast('[' .. username .. ']' .. " Cheat Deactive✅")
  gg.clearResults()
  silent()
end

function HOME()
  MultiMenu = gg.choice(
    { "🔥Silent Aim🔥", "⛔EXIT⛔" }, nil,
    "𝚂𝚒𝚕𝚎𝚗𝚝 𝚊𝚒𝚖 10 𝙳𝚎𝚐𝚛𝚎𝚎")
  if MultiMenu == nil then
    SAMPMENU = -1
  else
    if MultiMenu == 1 then silent() end
    if MultiMenu == 2 then EXIT() end
  end
  SAMPMENU = -1
end

while (true) do
  if gg.isVisible(true) then
     SAMPMENU = 1
     gg.setVisible(false)
     HOME()
 end
end
