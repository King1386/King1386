if Result[1] == Username and Result[2] == Password then
gg.toast('✅The password is correct✅')
else
gg.toast("❎The password is incorrect❎")
ggg = gg.alert("🌍The password is wrong. Please click Login and enter the correct password🧭", "🚀Login",nil,"🚫Exit")
if ggg == 1 then Login() else os.exit() end
end
if Result[3] then
	gg.saveVariable(Result, config)
else
	os.remove(config)
end
end
Login()

